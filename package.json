{
  "name": "inventarium-system",
  "version": "1.0.0",
  "description": "A system for managing inventories",
  "main": "app.js",
  "scripts": {
    "dev-start": "set NODE_ENV=development && nodemon --exec  babel-node app.js",
    "lint": "eslint ./server",
    "start": "babel-node app.js",
    "pretest": "npm run db-migrate",
    "test": "set NODE_ENV=test&& nyc --reporter=html --reporter=text mocha --require babel-register ./server/tests/ --timeout 15000 --exit",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "db-migrate": "set NODE_ENV=test&&babel-node server/models/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katiamiyu/inventarium-system.git"
  },
  "keywords": [
    "inventory",
    "inventarium",
    "system",
    "management"
  ],
  "author": "katiamiyu, james",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/katiamiyu/inventarium-system/issues"
  },
  "homepage": "https://github.com/katiamiyu/inventarium-system#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.3.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^7.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "pg": "^7.18.2",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^1.19.1"
  }
}
